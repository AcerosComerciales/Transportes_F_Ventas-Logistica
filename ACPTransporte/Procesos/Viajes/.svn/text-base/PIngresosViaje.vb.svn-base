   Private Sub chkGRecibos_CheckedChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles chkGRecibos.CheckedChanged
      grpRecibos.Enabled = chkGRecibos.Checked
   End Sub

   Private Sub btnGenerarRecibo_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles btnGenerarRecibo.Click
      Try
         If ACControles.ACDialogos.ACMostrarMensajePregunta(String.Format("Generar Recibo: {0}", Me.Text) _
                      , "¿Desea Generar el Recibo para el ingreso?" _
                      , ACControles.ACDialogos.LabelBotom.Si_No) = DialogResult.Yes Then
            managerTRAN_Recibos.TRAN_Recibos = New ETRAN_Recibos
            managerTRAN_Recibos.TRAN_Recibos.TIPOS_CodTipoRecibo = ETipos.getTipo(ETipos.TipoRecibo.Ingreso)
            managerTRAN_Recibos.TRAN_Recibos.RECIB_Serie = cmbSerie.SelectedValue
            managerTRAN_Recibos.TRAN_Recibos.RECIB_Numero = actxnNRecibo.Text
            managerTRAN_Recibos.TRAN_Recibos.RECIB_Codigo = String.Format("{0}{1}{2:0000000}", managerTRAN_Recibos.TRAN_Recibos.TIPOS_CodTipoRecibo.Substring(3, 1).PadLeft(2, "0") _
                                                                          , managerTRAN_Recibos.TRAN_Recibos.RECIB_Serie, managerTRAN_Recibos.TRAN_Recibos.RECIB_Numero)
            managerTRAN_Recibos.TRAN_Recibos.ZONAS_Codigo = GApp.Zona
            managerTRAN_Recibos.TRAN_Recibos.SUCUR_Id = GApp.Sucursal
            managerTRAN_Recibos.TRAN_Recibos.VIAJE_Id = managerTRAN_ViajesIngresos.TRAN_ViajesIngresos.VIAJE_Id
            managerTRAN_Recibos.TRAN_Recibos.TIPOS_CodTipoMoneda = cmbTipoMoneda.SelectedValue
            managerTRAN_Recibos.TRAN_Recibos.RECIB_Fecha = dtpFecha.Value
            managerTRAN_Recibos.TRAN_Recibos.RECIB_De = m_conductor
            managerTRAN_Recibos.TRAN_Recibos.RECIB_Monto = actxnMonto.Text
            managerTRAN_Recibos.TRAN_Recibos.RECIB_Concepto = String.Format("{0} / {1}", cmbFlete.Text, txtDescripcion.Text)

            If managerTRAN_ViajesIngresos.GuardarRecibo(GApp.Usuario, managerTRAN_Recibos.TRAN_Recibos) Then
               ACControles.ACDialogos.ACMostrarMensajeSatisfactorio(String.Format("Información: {0}", Me.Text), "Grabado satisfactoriamente")
               actxnNRecibo.ReadOnly = True
               btnGenerarRecibo.Enabled = False
               chkGRecibos.Enabled = False
               cmbSerie.Enabled = False
            Else
               ACControles.ACDialogos.ACMostrarMensajeInformacion("Información: " & Me.Text, "No puede guardar")
            End If
         End If
      Catch ex As Exception
         ACControles.ACDialogos.ACMostrarMensajeError(String.Format("Error: {0}", Me.Text), String.Format(Colecciones.getError("00101"), "Generando el recibo"), ex)
      End Try
   End Sub
End Class