#Region " Constructores "
   Public Sub New()

      ' This call is required by the Windows Form Designer.
      InitializeComponent()

      Try
         setInicio(Origen.Normal, DateTime.Now)
         grpRecibos.Visible = False
      Catch ex As Exception
         ACControles.ACDialogos.ACMostrarMensajeError(String.Format("Error: {0}", Convert.ToString(Me.Text)), "No se puede cargar los controles iniciales", ex)
      End Try
   End Sub

   Public Sub New(ByVal x_viajes_id As Long, ByVal x_fecha As DateTime)

      ' This call is required by the Windows Form Designer.
      InitializeComponent()

      Try
         setInicio(Origen.Viajes, x_fecha)
         m_etran_viajesingresos.VIAJE_Id = x_viajes_id
         cargarFletes(x_viajes_id)
         chkGRecibos.Enabled = True
         btnGenerarRecibo.Enabled = False
         grpRecibos.Enabled = False
      Catch ex As Exception
         ACControles.ACDialogos.ACMostrarMensajeError(String.Format("Error: {0}", Convert.ToString(Me.Text)), "No se puede cargar los controles iniciales", ex)
      End Try
   End Sub

   Public Sub New(ByVal x_vingr_id As Long, ByVal x_viajes_id As Long)

      ' This call is required by the Windows Form Designer.
      InitializeComponent()

      Try
         setInicio(Origen.Viajes, DateTime.Now)
         cargarFletes(x_viajes_id)
         If managerTRAN_ViajesIngresos.Cargar(x_viajes_id, x_vingr_id, True) Then
            acTool.setInstancia(ACControles.ACToolBarMantHorizontalNew.TipoInstancia.Nuevo)
            setInstancia(ACUtilitarios.ACSetInstancia.Nuevo)
            m_etran_viajesingresos = managerTRAN_ViajesIngresos.TRAN_ViajesIngresos
            AsignarBinding()
            grpRecibos.Enabled = False
            actxnNRecibo.Text = m_etran_viajesingresos.NumeroRecibo
            cmbSerie.SelectedValue = m_etran_viajesingresos.SerieRecibo
            btnGenerarRecibo.Enabled = Not (m_etran_viajesingresos.NumeroRecibo.Length > 0)
            chkGRecibos.Checked = (m_etran_viajesingresos.NumeroRecibo.Length > 0)
            chkGRecibos.Enabled = Not (m_etran_viajesingresos.NumeroRecibo.Length > 0)
            actxnNRecibo.ReadOnly = (m_etran_viajesingresos.NumeroRecibo.Length > 0)
            cmbSerie.Enabled = Not (m_etran_viajesingresos.NumeroRecibo.Length > 0)
         End If
      Catch ex As Exception
         ACControles.ACDialogos.ACMostrarMensajeError(String.Format("Error: {0}", Convert.ToString(Me.Text)), "No se puede cargar los controles iniciales", ex)
      End Try
   End Sub

   Private Sub setInicio(ByVal x_opcion As Origen, ByVal x_fecha As DateTime)
      Try
         tabMantenimiento.HideTabsMode = Crownwood.DotNetMagic.Controls.HideTabsModes.HideAlways
         tabMantenimiento.SelectedTab = tabBusqueda

         managerTRAN_ViajesIngresos = New BTRAN_ViajesIngresos
         managerAdministracionViajes = New BAdministracionViajes(GApp.Zona, GApp.Sucursal, GApp.PuntoVenta, GApp.Periodo)
         managerEntidades = New BEntidades
         managerTRAN_Recibos = New BTRAN_Recibos

         formatearGrilla()
         cargarCombos()
         m_opcion = x_opcion
         Select Case x_opcion
            Case Origen.Normal
               dtpFecha.Value = x_fecha
               grpRecibos.Visible = False
            Case Origen.Viajes
               acTool_ACBtnNuevo_Click(Nothing, Nothing)
               acTool.setInstancia(ACControles.ACToolBarMantHorizontalNew.TipoInstancia.Nuevo)
               dtpFecha.Value = x_fecha
            Case Else
               setInicio(Origen.Normal, DateTime.Now)
         End Select
         acTool.ACBtnEliminarEnabled = False
         acTool.ACBtnModificarEnabled = False
         chkComprobante.Checked = False
      Catch ex As Exception
         Throw ex
      End Try
   End Sub

