   Private Sub AsignarBinding()
      Try
         m_listBindHelper = New List(Of ACBindHelper)()
         m_listBindHelper.Add(ACBindHelper.ACBind(cmbTipoMoneda, "SelectedValue", m_etran_viajesingresos, "TIPOS_CodTipoMoneda"))
         m_listBindHelper.Add(ACBindHelper.ACBind(actxnMonto, "Text", m_etran_viajesingresos, "VINGR_Monto"))
         m_listBindHelper.Add(ACBindHelper.ACBind(txtDescripcion, "Text", m_etran_viajesingresos, "VINGR_Descripcion"))
         m_listBindHelper.Add(ACBindHelper.ACBind(cmbFlete, "SelectedValue", m_etran_viajesingresos, "FLETE_Id"))
         If m_etran_viajesingresos.VINGR_Fecha.Year < 1700 Then m_etran_viajesingresos.VINGR_Fecha = DateTime.Now
         m_listBindHelper.Add(ACBindHelper.ACBind(dtpFecha, "Value", m_etran_viajesingresos, "VINGR_Fecha"))
         '' Comprobante
         m_listBindHelper.Add(ACBindHelper.ACBind(actxaNroDocProveedor, "Text", m_etran_viajesingresos, "ENTID_Codigo"))
         m_listBindHelper.Add(ACBindHelper.ACBind(cmbGuia, "SelectedValue", m_etran_viajesingresos.TRAN_Documentos, "TIPOS_CodTipoDocumento"))
         m_listBindHelper.Add(ACBindHelper.ACBind(txtSerie, "Text", m_etran_viajesingresos.TRAN_Documentos, "DOCUS_Serie"))
         m_listBindHelper.Add(ACBindHelper.ACBind(actxnNumero, "Text", m_etran_viajesingresos.TRAN_Documentos, "DOCUS_Numero"))
      Catch ex As Exception
         Throw ex
      End Try
   End Sub

