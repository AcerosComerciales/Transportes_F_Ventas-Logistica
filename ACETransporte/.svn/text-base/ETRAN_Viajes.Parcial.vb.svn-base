			If Not IsNothing(m_viaje_scanfecha) Then
				If Not m_viaje_scanfecha.Equals(value) Then
					m_viaje_scanfecha = value
					OnVIAJE_ScanFechaChanged(m_viaje_scanfecha, EventArgs.Empty)
				End If
			Else
				m_viaje_scanfecha = value
				OnVIAJE_ScanFechaChanged(m_viaje_scanfecha, EventArgs.Empty)
			End If
		End Set
	End Property

