	Public Function Guardar(ByVal x_usuario As String) As Boolean
		Try
			If m_tran_neumaticos.Nuevo Then
				d_tran_neumaticos.TRAN_NEUMASI_UnReg(m_tran_neumaticos, x_usuario)
			ElseIf m_tran_neumaticos.Modificado Then
				d_tran_neumaticos.TRAN_NEUMASU_UnReg(m_tran_neumaticos, x_usuario)
			ElseIf m_tran_neumaticos.Eliminado Then
				d_tran_neumaticos.TRAN_NEUMASD_UnReg(m_tran_neumaticos)
			End If
			Return True
		Catch ex As Exception
			Throw ex
		End Try
	End Function
