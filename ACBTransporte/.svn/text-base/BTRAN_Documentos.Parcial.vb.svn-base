	Public Function Guardar(ByVal x_usuario As String) As Boolean
		Try
			If m_tran_documentos.Nuevo Then
				d_tran_documentos.TRAN_DOCUSSI_UnReg(m_tran_documentos, x_usuario)
			ElseIf m_tran_documentos.Modificado Then
				d_tran_documentos.TRAN_DOCUSSU_UnReg(m_tran_documentos, x_usuario)
			ElseIf m_tran_documentos.Eliminado Then
				d_tran_documentos.TRAN_DOCUSSD_UnReg(m_tran_documentos)
			End If
			Return True
		Catch ex As Exception
			Throw ex
		End Try
	End Function
