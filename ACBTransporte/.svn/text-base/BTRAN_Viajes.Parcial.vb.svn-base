	Public Function Guardar(ByVal x_usuario As String) As Boolean
		Try
			If m_tran_viajes.Nuevo Then
				d_tran_viajes.TRAN_VIAJESI_UnReg(m_tran_viajes, x_usuario)
			ElseIf m_tran_viajes.Modificado Then
				d_tran_viajes.TRAN_VIAJESU_UnReg(m_tran_viajes, x_usuario)
			ElseIf m_tran_viajes.Eliminado Then
				d_tran_viajes.TRAN_VIAJESD_UnReg(m_tran_viajes)
			End If
			Return True
		Catch ex As Exception
			Throw ex
		End Try
	End Function
